@{
    Layout = "";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Help - Tax App</title>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon -->
    <link href="/Theam/img/brand/favicon.png" rel="icon" type="image/png">
    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
    <!-- Icons -->
    <link href="/Theam/vendor/nucleo/css/nucleo.css" rel="stylesheet">
    <link href="/Theam/vendor/fortawesome/fontawesome-free/css/all.min.css" rel="stylesheet">
    <!-- Argon CSS -->
    <link type="text/css" href="/Theam/css/argon.css?v=1.0.0" rel="stylesheet">
</head>
<body>
    <div class="header bg-gradient-primary py-7 py-lg-8">
        <div class="container">
            <div class="header-body text-center mb-7">
                <div class="row justify-content-center">
                    <div class="col-lg-5 col-md-6">
                        <h1 class="text-white">Tax App</h1>
                        <a href="/Home/Index"
                           class="btn btn-white">Return Home</a>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="container-fluid mt--7">
        <div class="row">

            <div class="col-lg-12 col-md-12">
                <div class="card shadow">
                    <div class="card-header bg-transparent" data-toggle="collapse" href="#Hours">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">Dashboard</h3>
                            </div>
                            <div class="col text-right">
                                <i class="ni ni-bold-down"></i>
                            </div>
                        </div>
                    </div>
                    <div id="Hours" class="panel-collapse collapse">
                        <div class="card-body">
                            dashboard
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="col-lg-12 col-md-12">
                <div class="card shadow">
                    <div class="card-header bg-transparent" data-toggle="collapse" href="#Jobs">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">Jobs</h3>
                            </div>
                            <div class="col text-right">
                                <i class="ni ni-bold-down"></i>
                            </div>
                        </div>
                    </div>
                    <div id="Jobs" class="panel-collapse collapse">
                        <div class="card-body">
                            Jobs
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="col-lg-12 col-md-12">
                <div class="card shadow">
                    <div class="card-header bg-transparent" data-toggle="collapse" href="#Income">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">Income</h3>
                            </div>
                            <div class="col text-right">
                                <i class="ni ni-bold-down"></i>
                            </div>
                        </div>
                    </div>
                    <div id="Income" class="panel-collapse collapse">
                        <div class="card-body">
                            Income
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="col-lg-12 col-md-12">
                <div class="card shadow">
                    <div class="card-header bg-transparent" data-toggle="collapse" href="#Expenses">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">Expenses</h3>
                            </div>
                            <div class="col text-right">
                                <i class="ni ni-bold-down"></i>
                            </div>
                        </div>
                    </div>
                    <div id="Expenses" class="panel-collapse collapse">
                        <div class="card-body">
                            Expenses
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="col-lg-12 col-md-12">
                <div class="card shadow">
                    <div class="card-header bg-transparent" data-toggle="collapse" href="#Tax">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">Tax</h3>
                            </div>
                            <div class="col text-right">
                                <i class="ni ni-bold-down"></i>
                            </div>
                        </div>
                    </div>
                    <div id="Tax" class="panel-collapse collapse">
                        <div class="card-body">
                            Tax
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="col-lg-12 col-md-12">
                <div class="card shadow">
                    <div class="card-header bg-transparent" data-toggle="collapse" href="#Vat">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">VAT</h3>
                            </div>
                            <div class="col text-right">
                                <i class="ni ni-bold-down"></i>
                            </div>
                        </div>
                    </div>
                    <div id="Vat" class="panel-collapse collapse">
                        <div class="card-body">
                            Vat
                        </div>
                    </div>
                </div>
                <br />
            </div>

            <div class="col-lg-12 col-md-12">
                <div class="card shadow">
                    <div class="card-header bg-transparent" data-toggle="collapse" href="#Reports">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">Reports</h3>
                            </div>
                            <div class="col text-right">
                                <i class="ni ni-bold-down"></i>
                            </div>
                        </div>
                    </div>
                    <div id="Reports" class="panel-collapse collapse">
                        <div class="card-body">
                            Reports
                        </div>
                    </div>
                </div>
                <br />
            </div>

            </div>

        <hr />

        <!-- Footer -->
        <footer class="footer">
            <div class="row align-items-center justify-content-xl-between">
                <div class="col-xl-6">
                    <div class="copyright text-center text-xl-left text-muted">
                        <p>&copy; @DateTime.Now.Year - Tax App</p>
                    </div>
                </div>
                <div class="col-xl-6">
                    <ul class="nav nav-footer justify-content-center justify-content-xl-end">
                        <li class="nav-item">
                            <a href="/Home/Index" class="nav-link">Home</a>
                        </li>
                    </ul>
                </div>
            </div>
        </footer>
    </div>
</body>
</html>

<!-- Argon Scripts -->
<!-- Core -->
<script src="/Theam/vendor/jquery/dist/jquery.min.js"></script>
<script src="/Theam/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<!-- Optional JS -->
<script src="/Theam/vendor/chart.js/dist/Chart.min.js"></script>
<script src="/Theam/vendor/chart.js/dist/Chart.extension.js"></script>
<!-- Argon JS -->
<script src="/Theam/js/argon.js?v=1.0.0"></script>