                            </a>
                        }
                <div class="dropdown-menu dropdown-menu-arrow dropdown-menu-right" aria-labelledby="navbar-default_dropdown_1">
                    @foreach (var noti in ViewBag.NotificationList)
                    {
                        if (noti.notificationID == 0)
                        {
                            <a class='dropdown-item' href='#'>
                                @noti.Details.
                            </a>
                        }
                        else
                        {
                            <a class='dropdown-item'>
                                <div>
                                    <input type="button" class="btn btn-white" onclick="location.href='/Notifications/dismissNotifications?ID=@noti.notificationID&redirect=Redirect';" value="@noti.Details @noti.timeSince days since.">
