    <script>
var BarChart = (function() {

	//
	// Variables
	//

	var $chart = $('#chart-bar');
	var $ordersSelect = $('[name="ordersSelect"]');


	//
	// Methods
	//

	// Init chart
	function initChart($chart) {

		// Create chart
		var ordersChart = new Chart($chart, {
			type: 'bar',
			options: {
				scales: {
					yAxes: [{
						ticks: {
							callback: function(value) {
								if (!(value % 10)) {
                                    return '@Html.Raw(ViewBag.chartPrefix)'
                                        + value +
                                        '@Html.Raw(ViewBag.chartSufix)';
								}
							}
						}
					}]
				},
				tooltips: {
					callbacks: {
						label: function(item, data) {
							var label = data.datasets[item.datasetIndex].label || '';
							var yLabel = item.yLabel;
							var content = '';

							if (data.datasets.length > 1) {
								content += '<span class="popover-body-label mr-auto">' + label + '</span>';
							}

                            content += '<span class="popover-body-value">@Html.Raw(ViewBag.chartPrefix)'
                                + yLabel +
                                '@Html.Raw(ViewBag.chartSufix)</span>';

							return content;
						}
					}
				}
			},
			data: {
				labels: [@Html.Raw(ViewBag.chartLabels)],
				datasets: [{
					label: @Html.Raw(ViewBag.chartLabel),
					data: [@Html.Raw(@ViewBag.chartData)]
				}]
			}
		});

		// Save to jQuery object
		$chart.data('chart', ordersChart);
	}


	// Init chart
	if ($chart.length) {
		initChart($chart);
	}

        })();

        var LineChart = (function() {

	// Variables

	var $chart = $('#chart-line');


	// Methods

	function init($chart) {

		var salesChart = new Chart($chart, {
