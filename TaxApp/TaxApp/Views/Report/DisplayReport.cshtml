                                        <tr>
                                            @if (Model.column1Name != null)
                                            {
                                                <th scope="col">
                                                    <a href="/Report/DisplayReport?StartDateRange=@ViewBag.StartDateRange&EndDateRange=@ViewBag.EndDateRange&reportID=@ViewBag.reportID&SortBy=Col1&SortDirection=@ViewBag.SortDirection&DropDownID=@ViewBag.DropDownID">@Html.DisplayFor(model => model.column1Name)</a>
                                                </th>
                                            }
