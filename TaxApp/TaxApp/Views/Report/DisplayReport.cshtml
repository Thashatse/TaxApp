@model Model.ReportViewModel

@{
    ViewBag.Title = "Report";
    Layout = "";
}

<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title - Tax App</title>
    @Scripts.Render("~/bundles/modernizr")

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="author" content="Tax App">
    <!-- Favicon -->
    <link href="/Theam/img/brand/favicon.png" rel="icon" type="image/png">
    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
    <!-- Icons -->
    <link href="/Theam/vendor/nucleo/css/nucleo.css" rel="stylesheet">
    <link href="/Theam/vendor/fortawesome/fontawesome-free/css/all.min.css" rel="stylesheet">
    <!-- Argon CSS -->
    <link type="text/css" href="/Theam/css/argon.css?v=1.0.0" rel="stylesheet">

    <script>
        function search() {
            if (document.getElementById('serchTerm').value == null ||
                document.getElementById('serchTerm').value == "") {
                window.location.href = "/Home/Search?cat=@ViewBag.cat&term=" + document.getElementById('serchTermColapsed').value;
            }
            else {
                window.location.href = "/Home/Search?cat=@ViewBag.cat&term=" + document.getElementById('serchTerm').value;
            }
        }

    </script>
</head>
<body>
    <!-- Sidenav -->
