        <!-- Header -->
        <div class="header bg-gradient-primary py-7 py-lg-8">
            <div class="container">
                <div class="header-body text-center mb-7">
                    <div class="row justify-content-center">
                        <div class="col-lg-6 col-md-7">
                            <h1 class="text-white">
                                @Html.DisplayFor(model => model.reportTitle)
                            </h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="container-fluid mt--7">
            <div class="row mt-5">
                <div class="col-xl-12 mb-5 mb-xl-0">
                    <br />
                    <div class="card shadow">
                        <div class="card-header bg-transparent">
                            <div class="row align-items-center">
                                <div class="col-4">
                                    <h3 class="mb-0">Filter: </h3>
                                </div>
                                <div class="col-8 text-right">
                                    @using (Html.BeginForm())
                                    {
                                    <div class="row">

                                        @if ((ViewBag.AlsoShowDate == false || ViewBag.AlsoShowDate == null) && ViewBag.DropDownFilter != null)
                                        {
                                            <div class="col-lg-6 col-sm-12 text-right"></div>
                                        }
                                        @if (ViewBag.DropDownFilter != null)
                                        {
                                    <div class="col-lg-3 col-sm-12 text-right">
                                        @ViewBag.DropdownName:<br />
                                        @Html.DropDownList("DropDownFilter", ViewBag.DropDownFilter as SelectList)
                                    </div>
                                        }
                                        else
                                        {
                                            <div class="col-lg-3 col-sm-12 text-right"></div>
                                        }
                                        @if (ViewBag.AlsoShowDate == true)
                                        {
                                            <div class="col-lg-3 col-sm-12 text-right">
                                                Start Date: <input name="StartDate" type="date" value="@ViewBag.StartDateRange" />
                                            </div>
                                            <div class="col-lg-3 col-sm-12 text-right">
                                                End Date: <input name="EndDate" type="date" value="@ViewBag.EndDateRange" />
                                            </div>
                                        }
                                        <div class="col-lg-3 col-sm-12 text-right">

                                            @if (ViewBag.DropDownFilter != null || ViewBag.AlsoShowDate == true)
                                            {
                                                <br/>
                                                <input type="submit" value="Update" class="btn btn-default" />
                                            }
                                        </div>

                                    </div>
                                        }
                                    </div>
                            </div>
                        </div>
                    </div>
                    <br />
                </div>

                <!--Report-->
                @if (ViewBag.View == "")
                {
                    <div class="col-xl-12 mb-5 mb-xl-0">
